<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @restaurant.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @restaurant.address %>
</p>
<div>
  <% @restaurant.images.each do |upload|  %>
    <div class="image col-4 mb-4">
        <%= image_tag upload.photo.url %>
    </div>
  <% end %>
</div>
<%= link_to 'Edit', edit_restaurant_path(@restaurant) %> |
<%= link_to 'Back', restaurants_path %>
